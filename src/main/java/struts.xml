<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.devMode" value="true" />
    <constant name="struts.ui.theme" value="simple" />
    <constant name="struts.i18n.encoding" value="UTF-8"></constant>
    <package name="dongzijing" namespace="/" extends="struts-default">
        <global-results>
            <result name="error">/html/admin-404.html</result>
        </global-results>
        <action name="index" class="edu.csu.huangxinqi.action.PublicAction" method="execute" >
            <result name="login">/html/admin-index.html</result>
        </action>
    </package>
</struts>